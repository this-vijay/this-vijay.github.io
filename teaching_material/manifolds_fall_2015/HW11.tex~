\begin{document}
\title{Problem Set 11}
\date{November 6, 2015}
\author{Introduction to Manifolds}

\maketitle


\begin{prob}
Compute the de Rham cohomology ring of the $n$-sphere $S^n$.
\end{prob}

\begin{prob}
Compute the de Rham cohomology ring of the multiply-punctured Euclidean space $\R^n \setminus \{p_1, \ldots, p_m\}$,
where $m \geq 1$.
\end{prob}

\begin{prob}
 Compute the de Rham cohomology ring of the $n$-dimensional torus $T^n$.
\end{prob}

\begin{prob}
Compute the de Rham cohomology vector spaces of the real projective space $\R \P^n$.
\end{prob}

 \begin{prob}
Compute the de Rham cohomology vector spaces $H^k(\Sigma_g)$ for $k \geq 0$, where $\Sigma_g$ is the
compact orientable surface of genus $g$.
\end{prob}


\begin{prob}
An open cover of a manifold $M$ is a \emph{good cover} if every finite intersection of open sets is contractible.
Prove that if a manifold $M$ has a finite good cover, then its de Rham cohomology vector spaces $H^k(M)$ are finite dimensional
for all $k \geq 0$.
\end{prob}

\end{document}