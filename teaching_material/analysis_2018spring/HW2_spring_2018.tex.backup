\title{Problem Set 1}
\date{Jan 2, 2018}
\author{Analysis II}

\maketitle

\begin{prob}
Let $f$ and $g$ be integrable functions on $[a,b]$.
\begin{enumerate}[(a)]
 \item Show that if $P$ is any partition of $[a,b]$, then
 \[
 U(f+g,P) \leq U(f,P) + U(g,P).
 \]
 Provide a specific example where the inequality is strict.
 What does the corresponding inequality for lower sums
 look like?
 \item Prove that $\int_a^b (f + g) = \int_a^b f + \int_a^b g$.
\end{enumerate}
\end{prob}

\vspace{3mm}

\begin{prob}
Let $f$ and $g$ be integrable functions on $[a,b]$.  Prove that
the product $fg$ is also integrable on $[a,b]$ as follows:
\begin{enumerate}[(a)]
 \item If $f$ satisfies $|f(x)| \leq M$ on $[a,b]$, show
 \[
 |(f(x))^2 - (f(y))^2| \leq 2M|f(x) - f(y)|.
 \]
\end{enumerate}

\end{prob}


\vspace{3mm}


\begin{prob}
 Let $f: [a,b] \to \R$ be monotone increasing on the set $[a,b]$.
 Show that $f$ is integrable on $[a,b]$.
\end{prob}


\vspace{5mm}

*All questions taken from \emph{Understanding Analysis: 2nd Edition} by Stephen Abbott.


