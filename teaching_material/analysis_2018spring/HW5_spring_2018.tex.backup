\title{Problem Set 5}
\date{February 6, 2018}
\author{Analysis II}

\maketitle

\begin{prob}
Consider a function $f: \R^2 \to \R$ such that $f(0,0) = 0$ and $f(x,y) = h(x,y)$
for one of the following expressions $h$ when $(x,y) \neq (0,0)$.
Analyze the behavior of each function $f$ at $\+0 = (0,0)$.
In particular, determine for which vectors $\+u$ the directional derivative $f'(\+0,\+u)$ exists,
evaluate it for those $\+u$, determine $Df(\+0)$ (if it exists), and determine
whether $f$ is continuous at $\+0$.
\begin{enumerate}[(a)]
\vspace{2mm}
 \item $\frac{xy}{x^2 + y^2}$
\vspace{2mm}
 \item $\frac{x^2 y^2}{x^2y^2 + (y-x)^2}$
\vspace{2mm}
 \item $\frac{x^3}{x^2 + y^2}$
\vspace{2mm}
 \item $|x| + |y|$
\vspace{2mm}
 \item $|xy|^{1/2}$
\vspace{2mm}
 \item $\frac{x|y|}{\sqrt{x^2 + y^2}}$
 \end{enumerate}
\end{prob}

\vspace{2mm}

\begin{prob}
 For each of the following functions $f: \R^n \to \R^n$, 
 determine $Df$ and det$Df$, and sketch the image of the given set $S$ under $f$.
 \begin{enumerate}[(a)]
 \vspace{2mm}
  \item Let $f: \R^2 \to \R^2$, defined by $f(r, \theta) = (r \cos \theta, r \sin \theta)$. Let $S = [1,2] \times [0, \pi]$.
 \vspace{2mm}
 \item Let $f: \R^2 \to \R^2$, defined by $f(x,y) = (x^2 - y^2, 2xy)$.  Let $S = \{(x,y): x^2 + y^2 \leq a, x \geq 0, y \geq 0\}$,
  for some positive real number $a$.
  \vspace{2mm}
  \item Let $f: \R^2 \to \R^2$, defined by $f(x,y) = (e^x \cos y, e^x \sin y)$. Let $S = [0, 1] \times [0, \pi]$.
  \vspace{2mm}
  \item Let $f: \R^3 \to \R^3$, defined by $f(\rho, \phi, \theta) = (\rho \cos \theta \sin \phi, \rho \sin \theta \sin \phi, \rho \cos \phi)$.
  Let $S = [1,2] \times [0, \pi/2] \times [0,\pi/2]$.
 \end{enumerate}
\end{prob}



*All questions taken from \emph{Analysis on Manifolds} by James Munkres and \emph{Calculus on Manifolds} by Michael Spivak.


